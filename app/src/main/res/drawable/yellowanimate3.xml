    <item>
        <shape android:shape="rectangle" >
            <solid android:color="@color/black"/>
        </shape>
    </item>

    <item android:right="1dp" android:bottom="1dp" android:top="1dp" android:left="1dp">
        <shape android:shape="rectangle" >
            <solid android:color="@color/empty"/>
        </shape>
    </item>
    <item android:right="10dp" android:bottom="16dp" android:top="4dp" android:left="10dp">
        <shape  android:shape="oval" >
            <solid android:color="@color/yellow"/>
        </shape>
    </item>
</layer-list>